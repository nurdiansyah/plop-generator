{
  "ignorePatterns": [
    "coverage",
    "packages/*/index.*",
    "packages/*/libs",
    "node_modules",
    "**/*.svelte",
    "packages/*/build",
    "packages/*/dist"
  ],
  "extends": ["@nurdiansyah/devel"],
  "root": true,
  "rules": {
    "@typescript-eslint/no-empty-function": 0,
    "@typescript-eslint/no-var-requires": 0,
    "no-empty": 0
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  }
}
