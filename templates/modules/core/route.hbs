export const MODULE_NAME = "{{ projectName }}";

export type ModuleApiRoute = {
  /* GEN-ADD: type */
  {{ camelCase model }}Route: string;
  /* GEN-END: type */
};

export const getModuleApiRoute = (): ModuleApiRoute => {
  const base = MODULE_NAME;
  return {
    /* GEN-ADD: route */
    {{ camelCase model }}Route: `${base}/{{ camelCase model }}`
    /* GEN-END: route */
  };
};
