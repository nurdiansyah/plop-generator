{
  "name": "@deboxsoft/plop-generator",
  "version": "1.0.1-beta.35",
  "description": "Plop Generator to scaffold js/ts projects",
  "type": "module",
  "types": "libs",
  "bin": {
    "cre8-gen": "bin/cli.js"
  },
  "exports": {
    ".": "./libs/cli.js",
    "./libs/plopfile": "./libs/plopfile.js"
  },
  "scripts": {
    "test": "echo Error: no test specified",
    "start": "plop",
    "plop": "plop",
    "clean": "rimraf libs",
    "build": "pnpm clean && tsc",
    "postinstall": "node ./bin/postInstall.js",
    "bump": "pnpm run build && bump",
    "release:patch": "pnpm run bump -- patch -cp && pnpm publish",
    "release:minor": "pnpm run bump -- minor -cp && pnpm publish",
    "release:major": "pnpm run bump -- major -cp && pnpm publish",
    "release:dev": "git add . && pnpm run bump -- -cp prerelease && pnpm publish --no-git-checks"
  },
  "author": "Nurdiansyah <nurdiansyah>",
  "license": "SEE LICENSE",
  "dependencies": {
    "@deboxsoft/module-core": "^2.6.41",
    "dotenv": "^16.0.1",
    "edit-json-file": "^1.7.0",
    "minimist": "^1.2.6",
    "nconf": "^0.12.0",
    "nconf-yaml": "^1.0.2",
    "node": "^18.5.0",
    "node-plop": "^0.31.0",
    "plop": "^3.1.1"
  },
  "devDependencies": {
    "@babel/types": "^7.11.0",
    "@nurdiansyah/bump": "^6.1.2",
    "@nurdiansyah/eslint-config-devel": "^8.0.3",
    "tsup": "^6.2.3",
    "@types/babel__generator": "^7.6.1",
    "@types/edit-json-file": "^1.4.0",
    "@types/node": "^16.11.45",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "chalk": "^4.1.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "0.0.1-security",
    "inquirer": "^9.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "~4.7.3"
  },
  "files": [
    "bin",
    "plopfile.*",
    "core",
    "plugins",
    "libs",
    "types.*",
    "templates",
    "CODEOWNERS"
  ],
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  }
}
